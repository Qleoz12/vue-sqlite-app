{
  "name": "vue-sqlite-app",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "update": "npm install --save @capacitor-community/sqlite@latest --save-dev vue-sqlite-hook@latest --save-dev jeep-sqlite@latest",
    "serve": "npm run copysqlwasm && vue-cli-service serve",
    "build": "npm run copysqlwasm && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "copysqlwasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm public/assets"
  },
  "keywords": [
    "Vue",
    "Database",
    "SQLite",
    "Capacitor"
  ],
  "dependencies": {
    "@capacitor-community/electron": "^4.1.0",
    "@capacitor-community/sqlite": "^3.7.0",
    "@capacitor/android": "^3.7.0",
    "@capacitor/core": "^3.7.0",
    "@capacitor/ios": "^3.7.0",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@capacitor/cli": "^3.7.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "^2.4.1",
    "vue-sqlite-hook": "^2.1.8"
  },
  "description": "Vue App for @capacitor-community/sqlite",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jepiqueau/vue-sqlite-app.git"
  },
  "author": "Jean Pierre Qu√©au",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jepiqueau/vue-sqlite-app/issues"
  }
}
